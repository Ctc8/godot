[sub_resource type="GDScript" id="GDScript_vn42c"]
script/source = "extends Node

@onready var points_label = $SceneObjects/PointsLabel
@export var hearts : Array[Node]

var points = 0
var lives = 3

func decrease_health():
	lives = lives - 1
	print(lives)
	for h in 3:
		if (h < lives):
			hearts[h].show()
		else:
			hearts[h].hide()
	if (lives == 0):
		get_tree().reload_current_scene()

func add_point():
	points = points + 10
	print(points)
	points_label.text = \"Points: \" + str(points)
	#points_label.text = \"Points: \" + str(points)
"

